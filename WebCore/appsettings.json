{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://*:5000",
  "HeatingMonitor": {
    "SerialPortName": "/dev/ttyS0",
    "ExpectedReadIntervalInSeconds": 6
  },
  "HistoryService": {
    "SQLiteConnectionString": "Data Source=~\\History.db;",
    "SaveIntervalInMinutes": 10
  }
}